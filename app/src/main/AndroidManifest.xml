<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.tinlone.demo.rxjavasample">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CALL_LOG"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MEDIA_CONTENT_CONTROL"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".video.NetVideoActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_net_video"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme">
        </activity>

        <service android:name=".CallLogRebuildService"/>

        <activity android:name=".activity.MyActivity">
        </activity>
        <activity android:name=".activity.RecyclerviewActivity">
        </activity>
        <activity android:name=".activity.view.ViewATestActivity">
        </activity>
        <activity android:name=".activity.view.ViewBTestActivity">
        </activity>
        <activity android:name=".activity.view.ViewCTestActivity">
        </activity>
        <activity android:name=".test.Main2Activity">
        </activity>
    </application>

</manifest>